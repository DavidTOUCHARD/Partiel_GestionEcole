<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>GestionEcoleEnsup</display-name>
  <servlet>
  	<display-name>AddEtudiantServlet</display-name>
  	<servlet-name>AddEtudiantServlet</servlet-name>
  	<servlet-class>fr.ensup.gestionecole.presentation.AddEtudiantServlet</servlet-class>
  </servlet>
  <servlet>
  	<display-name>Inscription</display-name>
  	<servlet-name>Inscription</servlet-name>
  	<servlet-class>fr.ensup.gestionecole.presentation.Inscription</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddEtudiantServlet</servlet-name>
  	<url-pattern>/AddEtudiantServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Inscription</servlet-name>
  	<url-pattern>/Inscription</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  	<error-page>
		<error-code>404</error-code>
		<location>/404.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/500.jsp</location>
	</error-page>
</web-app>